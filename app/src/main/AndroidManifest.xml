<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nl.hr.tylerdegraaff.easyspot">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".LogInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BookingGeneralActivity" />
        <activity android:name=".BookingCampingActivity" />
        <activity android:name=".AccountActivity" />
        <activity android:name=".ReservationActivity" />

        <activity android:name=".CampingKaraokeActivity"></activity>

        <activity
            android:name=".BookingChoosePlotActivity"
            android:label="@string/title_activity_booking_choose_plot"
            android:theme="@style/AppTheme.NoActionBar"></activity>

    </application>

</manifest>